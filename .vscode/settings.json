{
  "eslint.useFlatConfig": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "files.encoding": "utf8",
  "files.autoGuessEncoding": true,
  "files.associations": {
    "*.json5": "json5",
  },
  "files.insertFinalNewline": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "antfu",
    "asdfrc",
    "composable",
    "Datetime",
    "dev",
    "devs",
    "dota",
    "dropoff",
    "elze",
    "esbuild",
    "fessional",
    "iconify",
    "imagetool",
    "ionicons",
    "jsdom",
    "movingkite",
    "npmjs",
    "nuxi",
    "nuxt",
    "nuxtjs",
    "nvmrc",
    "ofetch",
    "pinia",
    "pnpmfile",
    "rehook",
    "revi",
    "rimraf",
    "serv",
    "servs",
    "taze",
    "typecheck",
    "unocss",
    "vite",
    "vitejs",
    "vitest",
    "vnode",
    "vuetify",
    "vueuse"
  ],
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
  },
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "marscode.chatLanguage": "en",
  "marscode.codeCompletionPro": {
    "enableCodeCompletionPro": true
  },
  "marscode.enableInlineCommand": true,
  "marscode.enableCodelens": {
    "enableInlineDocumentation": true,
    "enableInlineUnitTest": true,
    "enableInlineExplain": false
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#1f6fd0",
    "titleBar.activeBackground": "#1857a4",
    "titleBar.activeForeground": "#e7e7e7",
    "activityBar.activeBackground": "#1f6fd0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ee90bb",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1f6fd0",
    "statusBar.background": "#1857a4",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1f6fd0",
    "statusBarItem.remoteBackground": "#1857a4",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1857a499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1857a4",
  "i18n-ally.sourceLanguage": "en-US",
  "i18n-ally.displayLanguage": "en-US",
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.localesPaths": ["**/i18n/locales/"],
  "i18n-ally.enabledParsers": ["json", "ts"],
  "i18n-ally.encoding": "utf-8",
  "i18n-ally.extract.keygenStyle": "camelCase",
  "i18n-ally.extract.keyMaxLength": 40,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": false,
  "i18n-ally.dirStructure": "file",
  "i18n-ally.fullReloadOnChanged": true,
}
